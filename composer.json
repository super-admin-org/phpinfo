{
    "name": "super-admin-org/phpinfo",
    "description": "an small plugin to show phpinfo right in your super-admin enviourment.",
    "type": "library",
    "keywords": ["super-admin", "extension"],
    "homepage": "https://github.com/super-admin-org/phpinfo",
    "license": "MIT",
    "authors": [
        {
            "name": "super-admin",
            "email": "infog@super-admin.org"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "super-admin-org/super-admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "SuperAdmin\\PHPInfo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SuperAdmin\\PHPInfo\\PHPInfoServiceProvider"
            ]
        }
    }
}
